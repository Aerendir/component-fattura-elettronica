    "type": "library",
    "license": "MIT",
    "require": {
        "php": "~7.2",
        "ext-SimpleXML": "^7.2",
        "ext-zip": "^1.15",
        "ext-dom": "20031129",
        "symfony/finder": "^4.2"
    },
    "require-dev": {
